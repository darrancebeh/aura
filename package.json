{
  "name": "aura-cli",
  "version": "0.1.1",
  "description": "Debug any blockchain transaction in seconds - CLI tool for analyzing Ethereum, Polygon, and Arbitrum transactions",
  "main": "dist/cli/index.js",
  "bin": {
    "aura": "dist/cli/index.js"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsx watch src/cli/index.ts",
    "build": "tsc",
    "start": "node dist/cli/index.js",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "polygon",
    "arbitrum",
    "transaction",
    "trace",
    "debug",
    "cli",
    "web3",
    "defi",
    "smart-contracts",
    "gas-analysis"
  ],
  "author": "darrancebeh",
  "license": "MIT",
  "homepage": "https://github.com/darrancebeh/aura#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darrancebeh/aura.git"
  },
  "bugs": {
    "url": "https://github.com/darrancebeh/aura/issues"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.0",
    "ethers": "^6.13.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "ts-jest": "^29.2.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  },
  "overrides": {
    "glob": "^10.0.0",
    "inflight": "npm:@isaacs/inflight@^1.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
